{
  "name": "@virginmediao2/storyblok-sdk",
  "version": "1.0.0",
  "description": "A TypeScript SDK for Storyblok CMS with Next.js compatibility",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "check": "biome check src/",
    "check:fix": "biome check --apply src/",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "storyblok",
    "cms",
    "typescript",
    "sdk",
    "api-client"
  ],
  "author": "Virgin Media O2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/virginmediao2/storyblok-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/virginmediao2/storyblok-sdk/issues"
  },
  "homepage": "https://github.com/virginmediao2/storyblok-sdk#readme",
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "axios": "^1.6.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
